page{
  background-color: #f5f5f5;
}
.content{
  width: 92%;
  margin: 0 auto;
  margin-top: 2vh;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.content_left{
  width: 30%;
  background-color:#ffffff;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.left_title{
  width: 100%;
  color: #ffffff;
  background-color:#FFC800;
  height: 6vh;
  text-align: center;
  line-height: 6vh;
  font-size: 40rpx;
}
.left_content{
  width: 100%;
  display: flex;
  align-items: center;
  flex-direction: column;
  height: 92vh;
  overflow-y: scroll;
}
.left_brand{
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  padding:20rpx 0;
  border-bottom: 2rpx solid #dcdcdc;
}
.left_brand image{
  width: 90rpx;
  height: 90rpx;
}
.content_right{
  background-color: #ffffff;
  height: 98vh;
  width: 68%;
  margin-left: 2%;
  overflow-y: scroll;
}
.right_title{
  width: 100%;
  color: #ffffff;
  background-color:#FFC800;
  height: 6vh;
  line-height: 6vh;
  font-size: 36rpx;
  padding-left: 30rpx;
  box-sizing: border-box;
}
.right_list_content{
  height: 110rpx;
  line-height:  110rpx;
  padding-left: 30rpx;
  box-sizing: border-box;
  font-size: 38rpx;
  border-bottom: 2rpx solid #dcdcdc;
}
.right_list_content:last-child{
  border-bottom: 0;
}