/* pages/login/login.wxss */
page {
  background-color: #ffffff;
}
.login_logo {
  width: 200rpx;
  height: 176rpx;
  display: block;
  margin:0 auto;
  margin-top: 160rpx;
}
.login_button {
  width: 400rpx;
  height: 90rpx;
  margin:0 auto;
  margin-top: 60rpx;
  background-color: #FEB400;
  font-size: 32rpx;
  color: #FFFFFF;
  text-align: center;
  line-height: 60rpx;
  border:none;
  border-radius:10rpx;
}
.login-btn::after {
  display: none;
}
.no_sq {
  font-size: 32rpx;
  border: none;
  line-height: 88rpx;
  background: #FEB400;
  color: #FFFFFF;
  font-size: 11pt;
  border-radius:10rpx;
  text-align: center;
  width: 400rpx;
  height: 90rpx;
  margin:0 auto;
  margin-top: 30rpx;
}